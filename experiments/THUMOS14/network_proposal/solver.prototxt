train_net: "train.prototxt"
test_net: "test.prototxt"
test_iter: 13682
test_interval: 5000
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 10000
# Display every 200 iterations
display: 200
# The maximum number of iterations
max_iter: 30000
# snapshot intermediate results
snapshot: 10000
snapshot_prefix: "snapshot/SCNN_uniform16_binary"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 0
